{
	"compilerOptions": {
		"target": "esnext", 
		"module": "commonjs", 
		"lib": [
			"es6", "es2017"
		],
		"allowJs": true,
		"jsx": "react-native",
		"noEmit": true,
		"isolatedModules": true,
		"strict": false,
		"resolveJsonModule": true,
		"moduleResolution": "node",
		"baseUrl": "./", 
		"paths": {
			"@environments/*": [
				"src/environments/*",
			],
			"@assets/*": [
				"src/assets/*",
			],
			"@pages/*": [
				"src/pages/*",
			],
			"@shared/*": [
				"src/shared/*",
			],
			"@components/*": [
				"src/shared/components/*",
			],
			"@models/*": [
				"src/shared/models/*",
			],
			"@services/*": [
				"src/shared/services/*",
			],
			"@styles/*": [
				"src/shared/styles/*",
			],
			"@translations/*": [
				"src/shared/translations/*",
			],
			"@routers/*": [
				"src/shared/routers/*",
			],
			"@base/*": [
				"./*",
			]
		},
	},
	"exclude": [
		"node_modules",
		"babel.config.js",
		"metro.config.js",
		"jest.config.js"
	]
}